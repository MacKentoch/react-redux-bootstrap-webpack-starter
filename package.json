{
  "name": "react-redux-bootstrap-webpack-starter",
  "version": "0.2.3",
  "description": "react js + redux + react router + hot reload + devTools + bootstrap + webpack starter",
  "main": "src/index.js",
  "scripts": {
    "dev": "NODE_ENV=dev webpack",
    "test": "NODE_ENV=nyc test mocha test/.setup.js test/**/*.spec.js --compilers js:babel-core/register --recursive",
    "coverage": "NODE_ENV=test nyc mocha test/.setup.js test/**/*.spec.js --compilers js:babel-core/register --recursive",
    "prod": "NODE_ENV=production webpack -p --config webpack.production.config.babel.js ",
    "start": "NODE_ENV=dev babel-node --presets es2015 ./server.hot.reload.js",

    "dev-win": "setx NODE_ENV=dev webpack",
    "test-win": "setx NODE_ENV=test nyc mocha test/.setup.js test/**/*.spec.js --compilers js:babel-core/register --recursive",
    "coverage-win": "setx NODE_ENV=test nyc mocha test/.setup.js test/**/*.spec.js --compilers js:babel-core/register --recursive",
    "prod-win": "setx NODE_ENV=production webpack -p --config webpack.production.config.babel.js ",
    "start-win": "setx NODE_ENV=dev babel-node --presets es2015 ./server.hot.reload.js"
  },
  "keywords": [
    "react",
    "redux",
    "react-redux",
    "ES6",
    "ES2015",
    "bootstrap",
    "starter",
    "webpack",
    "hot-reload",
    "redux-devtools",
    "devtools"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MacKentoch/react-redux-bootstrap-webpack-starter.git"
  },
  "bugs": {
    "url": "https://github.com/MacKentoch/react-redux-bootstrap-webpack-starter/issues"
  },
  "author": "Erwan DATIN (MacKentoch)",
  "license": "MIT",
