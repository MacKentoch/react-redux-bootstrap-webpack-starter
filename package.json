    "webpack-node-externals": "^1.7.2",
    "webpack-require-from": "^1.2.3",
    "workbox-webpack-plugin": "^3.1.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
