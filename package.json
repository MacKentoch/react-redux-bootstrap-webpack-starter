    "nodemon": "^1.17.3",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.0.0",
    "react-snap": "^1.12.2",
    "redux-devtools-extension": "^2.13.2",
