{
  "name": "react-redux-bootstrap-webpack-starter",
  "version": "6.0.0",
  "author": "Erwan DATIN (MacKentoch)",
  "license": "MIT",
  "description": "react js + redux + react router + hot reload + devTools + bootstrap + webpack starter",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MacKentoch/react-redux-bootstrap-webpack-starter.git"
  },
  "bugs": {
    "url": "https://github.com/MacKentoch/react-redux-bootstrap-webpack-starter/issues"
  },
  "engines": {
    "node": ">=8.11.1",
    "npm": ">=6.0.0",
    "yarn": ">=1.3.0"
  },
  "scripts": {
    "flow": "node_modules/.bin/flow",
    "test": "cross-env NODE_ENV=test jest",
    "predev": "rimraf docs/* &&  node ./scripts/prepareIndexHtml.js",
    "dev": "cross-env NODE_ENV=dev webpack --config webpack.dev.config.js",
    "postdev": "react-snap",
    "predev-static": "rimraf docs/* &&  node ./scripts/prepareIndexHtml.js",
    "dev-static": "cross-env NODE_ENV=dev webpack --config webpack.dev.config.js",
    "preprod": "rimraf docs/* && node ./scripts/prepareIndexHtml.js",
    "prod": "cross-env NODE_ENV=production webpack -p --config webpack.production.config.js",
    "postprod": "react-snap",
    "preprod-static": "rimraf docs/* &&  node ./scripts/prepareIndexHtml.js",
    "prod-static": "cross-env NODE_ENV=production webpack -p --config webpack.production.config.js",
    "analyze": "rimraf docs/assets && cross-env NODE_ENV=production webpack -p --config webpack.analyze.config.js",
    "prestart": "node ./scripts/prepareIndexHtml.js",
    "start": "cross-env NODE_ENV=dev webpack-dev-server --config ./webpack.hot.reload.config.js --mode development",
    "serve-dev": "npm run dev && nodemon ./src/server/server.js",
    "serve-prod": "npm run prod && node ./src/server/server.js",
    "serve-dev-static": "npm run dev-static && nodemon ./src/server/server.js",
    "serve-prod-static": "npm run prod-static && node ./src/server/server.js"
  },
  "reactSnap": {
    "source": "docs"
  },
  "keywords": [
    "react",
    "react 16",
    "redux",
    "react-redux",
    "ES6",
    "ES7",
    "ES2015",
    "ES2016",
    "bootstrap",
    "react-router4",
    "react-router",
    "starter",
    "webpack",
    "hot-reload",
    "redux-devtools-extension",
    "devtools",
    "webpack4"
  ],
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.17.0",
    "@types/chalk": "^2.2.0",
    "@types/classnames": "^2.2.3",
    "@types/compression": "^0.0.36",
    "@types/convict": "^4.2.0",
    "@types/date-fns": "^2.6.0",
    "@types/enzyme": "^3.1.10",
    "@types/express": "^4.11.1",
    "@types/express-promise-router": "^2.0.0",
